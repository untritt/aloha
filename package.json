{
  "name": "aloha",
  "description": "Homepage",
  "author": "Tuyana Dondokova <tuyana.dondokova@gmail.com>",
  "license": "0BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/untritt/aloha"
  },
  "bugs": {
    "url": "https://github.com/untritt/aloha/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "private": true,
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@configs/eslint": "*",
    "@configs/jest": "*",
    "@configs/ts": "*",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.9",
    "babel-preset-gatsby": "^0.5.5",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.4.1",
    "lerna": "^3.22.1",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx packages",
    "lint:dump": "eslint --print-config ./.eslintrc.json",
    "site:build": "yarn workspace @aloha/site run build",
    "site:dev": "yarn workspace @aloha/site run dev",
    "site:serve": "yarn workspace @aloha/site run serve",
    "site:clean": "yarn workspace @aloha/site run clean",
    "site:test": "yarn workspace @aloha/site run test",
    "theme:build": "yarn workspace @themes/gatsby-theme-aloha run build",
    "theme:dev": "yarn workspace @themes/gatsby-theme-aloha run dev",
    "theme:serve": "yarn workspace @themes/gatsby-theme-aloha run serve",
    "theme:clean": "yarn workspace @themes/gatsby-theme-aloha run clean",
    "theme:test": "yarn workspace @themes/gatsby-theme-aloha run test"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1"
  }
}
